{
  "name": "kirka",
  "version": "2.5.0",
  "description": "Monoids Rust-style for Typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/numfin/kirka.git"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.build.json",
    "test": "ava",
    "test:w": "ava -w",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "nextgen-samurai",
  "license": "ISC",
  "devDependencies": {
    "ava": "^5.2.0",
    "husky": "^8.0.3",
    "rimraf": "^4.3.1",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=tsx",
      "--no-warnings"
    ],
    "files": [
      "src/**/*test.ts"
    ]
  }
}

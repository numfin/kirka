{
  "name": "kirka",
  "version": "5.2.1",
  "description": "Monoids Rust-style for Typescript",
  "repository": {
    "type": "git",
    "url": "https://github.com/numfin/kirka.git"
  },
  "type": "module",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc -p tsconfig.build.json && npm run build:cjs",
    "build:cjs": "rollup dist/index.js --file dist/index.cjs --format cjs",
    "test": "ava",
    "test:w": "ava -w",
    "prepare:husky": "husky install"
  },
  "keywords": [],
  "author": "nextgen-samurai",
  "license": "ISC",
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "ava": "^6.1.3",
    "husky": "^9.0.11",
    "rimraf": "^5.0.7",
    "rollup": "^4.17.2",
    "typescript": "^5.4.5"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": "tsc"
    }
  },
  "engines": {
    "node": ">=20.5"
  }
}

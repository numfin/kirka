{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "strict": true,
    "declaration": true,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "outDir": "./dist",
    "declarationMap": true,
    "skipLibCheck": false
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "node_modules",
    "src/**/*test.ts",
    "src/iter/bak",
    "src/interfaces"
  ]
}
